<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="@dimen/activity_horizontal_margin"
android:paddingTop="@dimen/activity_vertical_margin"
android:paddingBottom="@dimen/activity_vertical_margin"
tools:context="com.vetrm.indoorpos.Settings$PlaceholderFragment">


<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Ok"
    android:id="@+id/btnOK"
    android:layout_alignParentBottom="true"
    android:layout_centerHorizontal="true"
    android:onClick="pressOk"
    />

<EditText
    android:layout_width="100dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/a1x"
    android:layout_alignParentTop="true"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true" />

<EditText
    android:layout_width="100dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/a2x"
    android:layout_alignParentTop="true"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true" />

<EditText
    android:layout_width="100dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/a3x"
    android:layout_alignParentTop="true"
    android:layout_centerHorizontal="true" />

<EditText
    android:layout_width="100dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/a1y"
    android:layout_below="@+id/a1x"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true" />

<EditText
    android:layout_width="100dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/a2y"
    android:layout_alignBottom="@+id/a1y"
    android:layout_centerHorizontal="true" />

<EditText
    android:layout_width="100dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/a3y"
    android:layout_alignBottom="@+id/a1y"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true" />

<EditText
    android:layout_width="100dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/a1z"
    android:layout_below="@+id/a1y"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true" />

<EditText
    android:layout_width="100dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/a2z"
    android:layout_alignBottom="@+id/a1z"
    android:layout_centerHorizontal="true" />

<EditText
    android:layout_width="100dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/a3z"
    android:layout_alignBottom="@+id/a1z"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true" />

<EditText
    android:layout_width="75dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/p1"
    android:layout_marginTop="45dp"
    android:layout_below="@+id/a2z"
    android:layout_centerHorizontal="true" />

<EditText
    android:layout_width="75dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/p2"
    android:layout_below="@+id/p1"
    android:layout_centerHorizontal="true" />

<EditText
    android:layout_width="75dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/p3"
    android:layout_below="@+id/p2"
    android:layout_centerHorizontal="true" />

<EditText
    android:layout_width="50dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/p1rest"
    android:layout_alignTop="@+id/p1"
    android:layout_toRightOf="@+id/p1"
    android:layout_toEndOf="@+id/p1" />

<EditText
    android:layout_width="50dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/p2rest"
    android:layout_below="@+id/p1rest"
    android:layout_alignLeft="@+id/p1rest"
    android:layout_alignStart="@+id/p1rest" />

<EditText
    android:layout_width="50dp"
    android:layout_height="wrap_content"
    android:inputType="number"
    android:ems="10"
    android:id="@+id/p3rest"
    android:layout_alignTop="@+id/p3"
    android:layout_alignLeft="@+id/p2rest"
    android:layout_alignStart="@+id/p2rest" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="New Text"
    android:id="@+id/p1before"
    android:layout_above="@+id/p2"
    android:layout_toStartOf="@+id/btnOK"
    android:layout_toLeftOf="@+id/btnOK" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="New Text"
    android:id="@+id/p2before"
    android:layout_above="@+id/p3"
    android:layout_toStartOf="@+id/p2"
    android:layout_alignRight="@+id/p1before"
    android:layout_alignEnd="@+id/p1before" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="New Text"
    android:id="@+id/p3before"
    android:layout_alignBottom="@+id/p3"
    android:layout_toStartOf="@+id/p3"
    android:layout_alignRight="@+id/p2before"
    android:layout_alignEnd="@+id/p2before" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="New Text"
    android:id="@+id/p1after"
    android:layout_above="@+id/p2rest"
    android:layout_toRightOf="@+id/p1rest"
    android:layout_toEndOf="@+id/p1rest" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="New Text"
    android:id="@+id/p2after"
    android:layout_above="@+id/p3rest"
    android:layout_toRightOf="@+id/p2rest"
    android:layout_toEndOf="@+id/p2rest" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="New Text"
    android:id="@+id/p3after"
    android:layout_alignBottom="@+id/p3rest"
    android:layout_toRightOf="@+id/p3rest"
    android:layout_toEndOf="@+id/p3rest" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="New Text"
    android:id="@+id/posy"
    android:layout_below="@+id/p3"
    android:layout_centerHorizontal="true"
    android:layout_marginTop="63dp" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="New Text"
    android:id="@+id/posz"
    android:layout_alignBottom="@+id/posy"
    android:layout_toEndOf="@+id/p3"
    android:layout_toRightOf="@+id/p3" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="New Text"
    android:id="@+id/posx"
    android:layout_alignBottom="@+id/posy"
    android:layout_toStartOf="@+id/p3"
    android:layout_toLeftOf="@+id/p3" />

<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Refresh"
    android:id="@+id/refresh"
    android:layout_below="@+id/posy"
    android:layout_alignLeft="@+id/btnOK"
    android:layout_alignStart="@+id/btnOK"
    android:onClick="refresh"
    />

    <ToggleButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New ToggleButton"
        android:id="@+id/updown"
        android:layout_below="@+id/refresh"
        android:layout_alignLeft="@+id/refresh"
        android:layout_alignStart="@+id/refresh"
        android:checked="false"
        android:onClick="revertUpDown"
        />

</RelativeLayout>
